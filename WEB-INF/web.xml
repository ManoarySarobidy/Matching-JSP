<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE web-app>
<web-app>

	<display-name>
		Exam today
	</display-name>

	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>servlet.Login</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>UserCreate</servlet-name>
		<servlet-class>servlet.UserServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>InfoAdd</servlet-name>
		<servlet-class>servlet.InfoServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>CritereAdd</servlet-name>
		<servlet-class>servlet.CriterialServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Match</servlet-name>
		<servlet-class>servlet.MatchServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>UserCreate</servlet-name>
		<url-pattern>/user</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>InfoAdd</servlet-name>
		<url-pattern>/add-Info</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>CritereAdd</servlet-name>
		<url-pattern>/add-Crit</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>Match</servlet-name>
		<url-pattern>/match</url-pattern>
	</servlet-mapping>

</web-app>